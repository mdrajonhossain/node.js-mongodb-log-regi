<form action="/auth" method="post">
	<div class="sign-up">
    	<h1 class="sign-up-title">Admin Login System</h1>
    	<input type="text" class="sign-up-input" name="username" placeholder="Username" required>
    	<input type="password" class="sign-up-input" name="password" placeholder="Password" required>
    	<input type="submit" value="Sign me up!" class="sign-up-button">
 <br/><br/>


 
  	</div>
</form>




